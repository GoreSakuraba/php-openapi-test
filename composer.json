{
  "name": "goresakuraba/php-openapi-test",
  "description": "A set of tools for testing your REST calls based on the Swagger or OpenAPI documentation using PHPUnit",
  "license": "MIT",
  "authors": [
    {
      "name": "Marius 'GoreSakuraba' Schiller",
      "email": "marius.schiller16@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/GoreSakuraba/php-openapi-test/graphs/contributors",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.5 || ^7.4",
    "guzzlehttp/psr7": "^1.9 || ^2.4"
  },
  "require-dev": {
    "slim/slim": "^4.10",
    "phpunit/phpunit": "^9.5",
    "slim/psr7": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "GoreSakuraba\\OpenAPI\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  }
}
