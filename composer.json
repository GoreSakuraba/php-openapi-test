{
  "name": "byjg/swagger-test",
  "description": "A set of tools for testing your REST calls based on the Swagger or OpenAPI documentation using PHPUnit",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "ext-curl": "*",
    "ext-json": "*",
    "guzzlehttp/psr7": "^2.4"
  },
  "require-dev": {
    "byjg/restserver": "4.0.*",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "ByJG\\ApiTools\\": "src/",
      "ByJG\\Util\\": "thirdparty/ByJG/Util"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  }
}
